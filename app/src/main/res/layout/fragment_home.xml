<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="uk.ac.uea.roomfinder.fragments.SearchFragment">

    <!-- TODO: Update blank fragment layout -->
    <Button
        android:text="BROWSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:id="@+id/browse_btn"
        android:layout_below="@+id/search_btn"
        android:layout_centerHorizontal="true"
        android:onClick="browseActivity" />

    <Button
        android:text="SEARCH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="135dp"
        android:id="@+id/search_btn"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/browse_btn"
        android:onClick="searchActivity"
        android:layout_alignEnd="@+id/browse_btn" />

</FrameLayout>
